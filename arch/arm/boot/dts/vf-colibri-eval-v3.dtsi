#include <dt-bindings/input/input.h>

/ {
	chosen {
		bootargs = "console=ttyLP0,115200";
		stdout-path = "serial0:115200n8";
	};

	clk16m: clk16m {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <16000000>;
	};

	bl_on {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_bl_on>;
		compatible = "gpio-backlight";
		gpios = <&gpio1 13 GPIO_ACTIVE_HIGH>;
		default-on;
	};

	extcon_usbc_det: usbc_det {
		compatible = "linux,extcon-usb-gpio";
		debounce = <25>;
#if 0
		id-gpio = <&gpio3 6 GPIO_ACTIVE_HIGH>;
#else		
		id-gpio = <&gpio2 25 GPIO_ACTIVE_HIGH>; /*SODIMM_133*/
#endif 	
		pinctrl-names = "default";
		/*pinctrl-0 = <&pinctrl_usbc_det>;*/
	};

	regulators {
		sys_5v0_reg: regulator@0 {
			compatible = "regulator-fixed";
			reg = <0>;
			regulator-name = "5v0";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
		};

		/* USBH_PEN */
								 
		usbh_vbus_reg: regulator@1 {
#if 1
			compatible = "regulator-fixed";
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_usbh1_reg>;
			reg = <1>;
			regulator-name = "usbh_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio3 12 GPIO_ACTIVE_HIGH>;
			vin-supply = <&sys_5v0_reg>;
#endif 
		};
		/*DAC8311 regulator*/	
		dac_vcc: fixedregulator@1 {
		      compatible = "regulator-fixed";
		      regulator-name = "fixed-supply";
		      regulator-min-microvolt = <2500000>;
		      regulator-max-microvolt = <2500000>;
		      regulator-always-on;
		       };
		touch_supply: fixedregulator@1 {
		      compatible = "regulator-fixed";
		      regulator-name = "fixed-supply";
		      regulator-min-microvolt = <2500000>;
		      regulator-max-microvolt = <2500000>;
		      regulator-always-on;
		       };
									
							 
	};

	gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpiokeys>;

		power {
			label = "Wake-Up";
			gpios = <&gpio1 9 GPIO_ACTIVE_HIGH>;
			linux,code = <KEY_WAKEUP>;
			debounce-interval = <10>;
			gpio-key,wakeup;
		};
	};
};

&bl {
	/*brightness-levels = <0 4 8 16 32 64 128 255>;*/
	max-brightness = <250>;
	/* default-brightness-level = <20>; */
	default-brightness-level = <25>; /* changed by Paul G 4/26/2017 */
	/*default-brightness-level = <8>; */ /* changed by Paul G 4/26/2017 */
/*	pwms = <&pwm0 0 50000 0>;*/
	pwms = <&pwm0 0 1250 0>;
	/*pwms = <&pwm0 0 100000 0>;*/
	status  = "okay";
};

&dcu0 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_dcu0_1>;
	
	display = <&display>;
	status = "okay";

	display: display@0 {
		bits-per-pixel = <16>;

		display-timings {
			native-mode = <&timing_vga>;
			/* WVGA Timing, TouchRevolution Fusion 7" */
			timing_wvga2: 800x480pixclkact {
				clock-frequency = <11000000>;
				hactive = <800>;
				vactive = <600>;
				hback-porch = <216>;
				hfront-porch = <40>;
				vback-porch = <35>;
				vfront-porch = <10>;
				hsync-len = <128>;
				vsync-len = <2>;
				hsync-active = <0>;
				vsync-active = <0>;
				pixelclk-active = <1>;
			};
			/* Standard VGA timing */
			timing_vga: 640x480 {
				clock-frequency = <25175000>;
				hactive = <640>;
				vactive = <480>;
				hback-porch = <40>;
				hfront-porch = <24>;
				vback-porch = <32>;
				vfront-porch = <11>;
				hsync-len = <96>;
				vsync-len = <2>;
				hsync-active = <0>;
				vsync-active = <0>;
				pixelclk-active = <0>;
			};
			/* new settings - Setra Display */
			/* WVGA Timing, e.g. EDT ET070080DH6 */
			timing_wvga: 800x480 {
				clock-frequency = <33000000>;
				hactive = <800>;
				vactive = <480>;
				hback-porch = <216>;
				hfront-porch = <40>;
				vback-porch = <35>;
				vfront-porch = <10>;
				hsync-len = <128>;
				vsync-len = <2>;
				hsync-active = <0>;
				vsync-active = <0>;
				pixelclk-active = <0>;
			};
			/* Standard SVGA timing */
			timing_svga: 800x600 {
				clock-frequency = <40000000>;
				hactive = <800>;
				vactive = <600>;
				hback-porch = <88>;
				hfront-porch = <40>;
				vback-porch = <23>;
				vfront-porch = <1>;
				hsync-len = <128>;
				vsync-len = <4>;
				hsync-active = <1>;
				vsync-active = <1>;
				pixelclk-active = <0>;
			};
			/* TouchRevolution Fusion 10"/CLAA101NC05 10.1 inch */
			timing_wsvga: 1024x600 {
				clock-frequency = <48000000>;
				hactive = <1024>;
				vactive = <600>;
				hback-porch = <104>;
				hfront-porch = <43>;
				vback-porch = <24>;
				vfront-porch = <20>;
				hsync-len = <5>;
				vsync-len = <5>;
				hsync-active = <0>;
				vsync-active = <0>;
				pixelclk-active = <0>;
			};
			/* Standard XGA timing */
			timing_xga: 1024x768 {
				clock-frequency = <65000000>;
				hactive = <1024>;
				vactive = <768>;
				hback-porch = <160>;
				hfront-porch = <24>;
				vback-porch = <29>;
				vfront-porch = <3>;
				hsync-len = <136>;
				vsync-len = <6>;
				hsync-active = <0>;
				vsync-active = <0>;
				pixelclk-active = <0>;
			};
		};
	};
};

/*Kiran Bhosale  addition of Device tree nodes for  AD7798 ADC & DAC8311 DAC*/
&dspi1 {
	status = "okay";
   ad5446@0 {
		 compatible = "adi,dac8311";
		 reg = <0>;
		 spi-max-frequency = <4000000>; /*5 MHz Clock */
		 /*spi-max-frequency = <25000000>; /*25 MHz Clock */
		 spi-cpha; /*This is  required by the DAC311 is CPOL = 0 & CPHASE =1 */
		 vcc-supply = <&sys_3v3_avdd_reg>;
	 };
ad7798: ad@1 {
		compatible = "adi,ad7798";
		pinctrl-names = "default";
		/*pinctrl-0 = <&pinctrl_can_int>;*/
		reg = <1>;
		clocks = <&clk16m>;
		spi-max-frequency = <4000000>; /*5  MHz ADC maximum supported is 50 Mhz  */
		/*spi-max-frequency = <5000000>; /*25  MHz ADC maximum supported is 50 Mhz  */
		/*interrupt-parent = <&gpio1>;*/
   spi-cpol; /*This is  required by the AD7798* i.e CPOL = 1 CPHASE = 1*/
   spi-cpha; 
		/*interrupts = <12 GPIO_ACTIVE_LOW>;*/
	};
};

&esdhc1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_esdhc1>;
	bus-width = <4>;
	status = "okay";
};

&fec1 {
	phy-mode = "rmii";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_fec1>;
	status = "okay";
};

